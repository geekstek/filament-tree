{
    "name": "geekstek/filament-tree",
    "description": "Tree form components for Filament v4",
    "keywords": [
        "filament",
        "laravel",
        "tree",
        "tree-select",
        "cascading-select"
    ],
    "homepage": "https://github.com/geekstek/filament-tree",
    "license": "MIT",
    "authors": [
        {
            "name": "Willie Ong",
            "email": "willie@example.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "filament/filament": "^4.0",
        "spatie/laravel-package-tools": "^1.16"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "nunomaduro/collision": "^8.1",
        "orchestra/testbench": "^10.0",
        "pestphp/pest": "^3.0",
        "pestphp/pest-plugin-livewire": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Geekstek\\FilamentTree\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Geekstek\\FilamentTree\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "Geekstek\\FilamentTree\\FilamentTreeServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
